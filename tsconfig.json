{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": ["node", "express", "cors", "dotenv", "mongoose"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
